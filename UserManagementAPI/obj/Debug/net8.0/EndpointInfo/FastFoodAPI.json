        "properties": {
          "name": {
            "maxLength": 150,
            "minLength": 2,
            "type": "string"
          },
          "price": {
            "maximum": 1000000,
            "minimum": 0,01,
            "type": "number",
            "format": "double"
          },
          "categoryId": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "imageUrl": {
            "type": "string",
            "format": "uri",
